git config --global user.name lmurta
git config --global user.email xx@yy

git config --global credential.helper 'cache --timeout=3600'

github > create repository

git add .
git remote add origin https://github.com/lmurta/analogRead
git push origin master
git pull origin master

on new computer
git clone https://github.com/lmurta/analogRead
git add --all

after file modified
git commit -am "file modified"
git push
